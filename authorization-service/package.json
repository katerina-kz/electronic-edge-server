    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "esbuild": "^0.14.11",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.0.0",
    "serverless-webpack": "^5.11.0",
    "ts-node": "^10.4.0",
    "ts-loader": "^8.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^1.7.2"
