{
  "name": "product-service-ts",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.yml",
  "scripts": {
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "sls:invoke": "sls invoke local --function catalogBatchProcess --path path.json",
    "test": "jest --watchAll --config=jest.config.js",
    "watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^3.4.0",
    "@middy/http-cors": "^4.2.5",
    "@middy/http-json-body-parser": "^3.4.0",
    "dotenv": "^16.0.3",
    "pg": "^8.9.0"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.284.0",
    "@aws-sdk/lib-dynamodb": "^3.284.0",
    "aws-sdk-mock": "^5.8.0",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "jest": "^29.4.3",
    "json-schema-to-ts": "^1.5.0",
    "lambda-tester": "^4.0.1",
    "serverless": "^3.0.0",
    "serverless-auto-swagger": "^2.12.0",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.5",
    "uuid": "^9.0.0",
    "webpack": "^4.29.0",
